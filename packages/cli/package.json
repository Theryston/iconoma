{
  "name": "@iconoma/cli",
  "description": "A developer-friendly tool to manage and organize icons through CLI and web studio",
  "version": "0.0.0",
  "author": "Theryston Santos @Theryston",
  "bin": {
    "iconoma": "./bin/run.js"
  },
  "bugs": "https://github.com/theryston/iconoma/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "figlet": "^1.9.4",
    "gradient-string": "^3.0.0",
    "nanospinner": "^1.2.2",
    "open": "^11.0.0"
  },
  "devDependencies": {
    "@iconoma/eslint-config": "workspace:*",
    "@iconoma/typescript-config": "workspace:*",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/theryston/iconoma",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "iconoma",
    "dirname": "iconoma",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "theryston/iconoma",
  "scripts": {
    "postbuild": "node scripts/bundle-studio.js",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
