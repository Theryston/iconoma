{
  "name": "@iconoma/studio",
  "version": "0.0.5",
  "type": "module",
  "main": "dist/server.js",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theryston/iconoma.git"
  },
  "homepage": "https://github.com/theryston/iconoma",
  "keywords": [
    "oclif",
    "iconoma",
    "icon management",
    "icons",
    "studio",
    "CLI",
    "SVG",
    "react",
    "icon pipeline",
    "icon optimizer",
    "icon library",
    "SVGO",
    "design system",
    "icon build",
    "icon generator",
    "react icons"
  ],
  "license": "MIT",
  "author": "Theryston Santos @Theryston",
  "scripts": {
    "dev": "nodemon --watch server.ts --watch ./api --ext ts,js,tsx --exec pnpm run dev:server",
    "dev:server": "tsx server.ts",
    "build": "tsc -p tsconfig.server.json && ts-add-js-extension --dir=dist && pnpm build:client && pnpm build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.90.12",
    "@vitejs/plugin-react": "^5.1.2",
    "axios": "^1.13.2",
    "canvas-confetti": "^1.9.4",
    "compression": "^1.8.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "fastq": "^1.19.1",
    "get-port": "^7.1.0",
    "lucide-react": "^0.475.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.68.0",
    "react-router": "^7.11.0",
    "sirv": "^3.0.2",
    "svgo": "^4.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@iconoma/eslint-config": "workspace:^",
    "@iconoma/typescript-config": "workspace:*",
    "@types/canvas-confetti": "^1.9.0",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.6",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "nodemon": "^3.1.11",
    "ts-add-js-extension": "^1.6.6",
    "vite": "^7.3.0",
    "tsx": "^4.21.0"
  },
  "exports": {
    ".": "./dist/server.js"
  }
}
