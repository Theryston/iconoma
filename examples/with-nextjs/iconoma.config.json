{
  "svg": {
    "folder": "./icons",
    "inLock": false
  },
  "svgo": {
    "plugins": [
      "removeDimensions",
      "removeHiddenElems",
      "removeTitle",
      {
        "name": "convertColors",
        "params": {
          "currentColor": true
        }
      }
    ]
  },
  "extraTargets": [
    {
      "targetId": "react",
      "outputPath": "./components/icons.tsx"
    }
  ]
}