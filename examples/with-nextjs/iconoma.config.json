{
  "svg": {
    "folder": "./icons",
    "inLock": false
  },
  "svgo": {
    "plugins": [
      "removeDimensions",
      "removeHiddenElems",
      "removeTitle",
      {
        "name": "convertColors",
        "params": {
          "currentColor": false,
          "names2hex": true
        }
      }
    ]
  },
  "extraTargets": [
    {
      "targetId": "react",
      "outputPath": "./components/icons.tsx"
    },
    {
      "targetId": "react-native",
      "outputPath": "./react-native/icons/{name}.tsx"
    }
  ],
  "colorVariables": [
    "--icons-secondary"
  ]
}