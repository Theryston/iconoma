{
  "svg": {
    "folder": null,
    "inLock": true
  },
  "svgo": {
    "plugins": [
      "removeDimensions",
      "removeHiddenElems",
      {
        "name": "preset-default",
        "params": {
          "overrides": {
            "removeTitle": false
          }
        }
      },
      {
        "name": "convertColors",
        "params": {
          "currentColor": true
        }
      }
    ]
  },
  "extraTargets": [
    {
      "targetId": "react",
      "outputPath": "./components/icons.tsx"
    }
  ]
}