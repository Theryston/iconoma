{
  "name": "@iconoma/studio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch server.ts --watch ./api --ext ts,js,tsx --exec pnpm run dev:server",
    "dev:server": "tsx server.ts",
    "build": "pnpm build:client && pnpm build:server && tsc -p tsconfig.server.json && ts-add-js-extension --dir=dist",
    "postbuild": "ncc build ./dist/server.js -o .bundle && node ./scripts/copy-client.mjs",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@iconoma/ui": "workspace:*",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.90.12",
    "@vitejs/plugin-react": "^5.1.2",
    "compression": "^1.8.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "get-port": "^7.1.0",
    "lucide-react": "^0.475.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.11.0",
    "sirv": "^3.0.2"
  },
  "devDependencies": {
    "@iconoma/eslint-config": "workspace:^",
    "@iconoma/typescript-config": "workspace:*",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.6",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vercel/ncc": "^0.38.4",
    "nodemon": "^3.1.11",
    "ts-add-js-extension": "^1.6.6",
    "tsx": "^4.21.0",
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "exports": {
    ".": "./dist/server.js"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.5"
    }
  }
}
